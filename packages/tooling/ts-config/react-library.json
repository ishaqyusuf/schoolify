{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "target": "es2017",
    "jsx": "react-jsx",
    "noEmit": true,
    "noImplicitAny": false
  },
  "exclude": ["node_modules"]
}
